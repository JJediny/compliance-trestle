{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "IBM Cloud Compliance & Security Scan Summary Results",
  "version": "0.1",
  "type": "object",
  "properties": {
    "profile": { "type": "string" },
    "control_number": { "type": "string" },
    "description": { "type": "string" },
    "severity": { "type": "string" },
    "ticket_id": { "type": "string" },
    "overridden_till": { "type": "string", "format": "date-time" },
    "reason_for_override": { "type": "string" },
    "result": { "type": "string", "pattern": "PASS|FAIL" },
    "passed": { "type": "integer" },
    "failed": { "type": "integer" },
    "unable_to_perform" : { "type": "integer" },
    "not_applicable": { "type": "integer" },
    "total": { "type": "integer" }
  },
  "required": [
    "profile",
    "control_number",
    "description",
    "severity",
    "result",
    "passed",
    "failed",
    "unable_to_perform",
    "not_applicable",
    "total"
  ]
}
